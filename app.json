{
  "expo": {
    "name": "github-stats",
    "slug": "github-stats",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/github.png",
    "scheme": "githubstats",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icons/github.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.niharhegde.githubstats"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/github.png",
        "backgroundColor": "#000000"
      },
      "package": "com.niharhegde.githubstats"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/github.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "newArchEnabled": true
    }
  }
}
