{
  "expo": {
    "name": "github-stats",
    "slug": "github-stats",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/github.png",
    "scheme": "github-stats",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/github.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/github.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000",
          "enableFullScreenImage_legacy": true
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
